%ul{id: 'board', title: @board["Title"], selected: "true"}
  %li{class: "group"} Lanes
  - @lanes.each do |lane|
    %li
      %a{href: "#lane_#{lane["Id"]}"} 
        =lane["Title"]
- @lanes.each do |lane|
  %div{id: "lane_#{lane["Id"]}", title: lane["Title"] }
    %ul
      - lane["Cards"].each do |card|
        %li
          %a{href: "#card_#{card["Id"]}"} 
            =card["Title"]
